  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "esbuild": "^0.14.48",
    "fs-extra": "^10.1.0",
    "process-argv": "^1.0.0",
    "protobufjs": "^6.11.3"
  }
}
