{
  "name": "simplepbjava",
  "version": "1.0.0",
  "description": "generate simple pb java",
  "main": "index.js",
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "gen": "node ./build/cli.js build ./protos/namecard.proto",
